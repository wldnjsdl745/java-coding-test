import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int num1 = sc.nextInt();
        String num2 = sc.next();

        String[] arr = num2.split("");

        int sum = 0;
        for (int i = 0; i < num1; i++) {
            sum += Integer.parseInt(arr[i]);
        }

        System.out.println(sum);
    }
}
